pipeline {
    agent any

    stages {
        stage('git clone') {
            steps {
                sleep(10)
                echo 'git clone'
            }
        }
        stage('build') {
            steps {
                echo 'build'
            }
        }
        stage('test') {
            steps {
                echo 'test'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy'
            }
        }
    }
}

